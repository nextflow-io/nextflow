docker.enabled = true
process.container = 'nextflow/stress'

trace {
  enabled = true
  fields =  ['task_id',
            'hash',
            'native_id',
            'name',
            'status',
            'exit',
            'submit',
            'duration',
            'realtime',
            '%cpu',
            'rss',
            'peak_rss',
            'peak_vmem',
            'vmem',
            'rchar',
            'wchar',
            'read_bytes',
            'write_bytes']
}
